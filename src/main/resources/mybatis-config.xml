		<typeAlias type="global.sesoc.www.dto.T_GRequest"	alias="grequest" />
		<typeAlias type="global.sesoc.www.dto.T_Message"	alias="message" />
		<typeAlias type="global.sesoc.www.dto.T_Reply"	alias="reply" />
      
	</typeAliases>

	<mappers>
    
		<mapper resource="mappers/T_BoardMapper.xml"/>
		<mapper resource="mappers/T_GroupMapper.xml"/>
		<mapper resource="mappers/T_PlannerMapper.xml"/>
		<mapper resource="mappers/T_PlistMapper.xml"/>
		<mapper resource="mappers/T_RequestMapper.xml"/>
		<mapper resource="mappers/T_ScheduleMapper.xml" />
		<mapper resource="mappers/T_UserMapper.xml"/>
		<mapper resource="mappers/T_FriendMapper.xml" />
		<mapper resource="mappers/T_GRequestMapper.xml" />
		<mapper resource="mappers/T_MessageMapper.xml" />
		<mapper resource="mappers/T_ReplyMapper.xml" />
    
	</mappers> 

</configuration>